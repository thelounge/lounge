{
  "name": "thelounge",
  "description": "The self-hosted Web IRC client",
  "version": "2.0.0-rc.1",
  "publishConfig": {
    "tag": "next"
  },
  "preferGlobal": true,
  "bin": {
    "lounge": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thelounge/lounge.git"
  },
  "homepage": "https://thelounge.github.io/",
  "scripts": {
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -r test/fixtures/env.js test/**/*.js",
    "start": "node index",
    "build": "NODE_ENV=production jsio-webpack",
    "watch": "jsio-webpack --watch",
    "serve": "jsio-webpack serve --hot",
    "build:font-awesome": "node scripts/build-fontawesome.js",
    "build:grunt": "grunt",
    "build:handlebars": "handlebars client/views/ -e tpl -f client/js/lounge.templates.js",
    "build:webpack": "jsio-webpack",
    "watch:webpack": "jsio-webpack -w",
    "test": "npm-run-all -c test:mocha lint",
    "test:mocha": "mocha -r test/fixtures/env.js test/**/*.js",
    "lint": "npm-run-all -c lint:js lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint \"**/*.css\""
  },
  "keywords": [
    "browser",
    "web",
    "chat",
    "client",
    "irc",
    "server"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4.2.0"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bootstrap": "^3.3.7",
    "bootstrap-webpack": "0.0.5",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "event-stream": "3.3.2",
    "express": "4.13.4",
    "font-awesome-webpack": "0.0.4",
    "fs-extra": "0.30.0",
    "irc-framework": "2.5.0",
    "irc-style-parser": "^0.2.4",
    "jquery": "^3.1.1",
    "lato-font": "^1.1.1",
    "ldapjs": "1.0.0",
    "less": "^2.7.1",
    "lodash": "4.11.2",
    "moment": "2.13.0",
    "primer-tooltips": "^0.4.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "read": "1.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "request": "2.74.0",
    "semver": "5.1.0",
    "socket.io": "1.4.5",
    "socket.io-client": "^1.4.8",
    "spdy": "3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "chai": "3.5.0",
    "classnames": "2.2.5",
    "enzyme": "2.4.1",
    "eslint": "2.11.1",
    "eslint-plugin-react": "6.3.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "font-awesome": "4.6.3",
    "grunt": "1.0.1",
    "grunt-contrib-uglify": "1.0.1",
    "grunt-contrib-watch": "1.0.0",
    "handlebars": "4.0.5",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "istanbul": "0.4.3",
    "jest": "15.1.1",
    "jsio-webpack": "git+https://github.com/jsio-private/jsio-webpack",
    "mocha": "2.4.5",
    "npm-run-all": "2.1.1",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "2.1.0",
    "stylelint": "6.6.0",
    "webpack": "1.13.2"
  }
}
